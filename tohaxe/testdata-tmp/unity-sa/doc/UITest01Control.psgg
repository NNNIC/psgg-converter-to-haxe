/*
    [[state]]
    [[state-cmt]]
*/
void [[state]](bool bFirst)
{
    if (bFirst)
    {
        [[init]]

        <<<?parts
        create("[[parts]]","[[ref]]");
        >>>

        <<<?anchor
        set_anchor("[[anchor]]");
        >>>

        <<<?pivot
        set_pivot("[[pivot]]");
        >>>

        <<<?size
        set_size([[size]]);
        >>>

        <<<?pos
        set_pos([[pos]]);
        >>>

        <<<?text
        set_text("[[text]]");
        >>>

        <<<?textclr
        set_text_clr();
        >>>

        <<<?sprite
        set_sprite("[[sprite]]");
        >>>
    }

    [[update]]

    <<<?wait
    if (![[wait]]) return;
    >>>

    [[post_wait]]

    [[branch]]

    <<<?nextstate
    if (!HasNextState())
    {
        SetNextState([[nextstate]]);
    }
    >>>

    if (HasNextState())
    {
        <<<?nowait
            NoWait();
        >>>

        GoNextState();
    }
}
###VARIOUS-CONTENTS-END###


